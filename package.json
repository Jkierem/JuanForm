{
  "name": "juanform",
  "version": "2.2.5",
  "description": "React Form Components",
  "author": "jkierem",
  "license": "MIT",
  "main": "juanform/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jkierem/JuanForm"
  },
  "scripts": {
    "start": "concurrently \"npm run lib:watch\"  \"npm run docs\"",
    "lib": "babel src/juanform -d juanform --copy-files",
    "lib:watch": "babel src/juanform -w -d juanform --copy-files",
    "docs": "webpack-dev-server --mode development",
    "docs:prod": "webpack --mode production",
    "test": "rimraf ./coverage && jest --coverage -u",
    "test:watch": "jest -u --watch"
  },
  "peerDependencies": {
    "react": "~16.4.1",
    "react-dom": "~16.4.1",
    "styled-components": "^3.4.9"
  },
  "keywords": [
    "form",
    "react"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-styled-components": "^1.7.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "jsdom": "^13.2.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "rimraf": "^2.6.3",
    "sinon": "^7.2.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.3"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties",
      "styled-components"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/juanform/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)test.{js,jsx}"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  }
}
